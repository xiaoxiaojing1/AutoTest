  {
    "description":"这是返回cookies的get请求",
    "request":{
      "uri":"/getCookies",
      "method":"get"
    },
    "response":{
      "text":"恭喜你获得cookies成功",
      "cookies":{"login":"true","status":"100002"}
    }
  },

  {
    "description":"这是xiaoxiaojing的第一moco接口啊",
    "request":{
      "uri":"/xxj"
    },
    "response":{
      "text":"返回xiaoxiaojing啊"
    }
  },

  {
    "description": "这是一个带参数的get请求啊",
    "request": {
      "uri": "/get",
      "method": "get",
      "queries": {
        "name": "xiaoxiaojing",
        "age": "27"
      }
    },
    "response": {
      "text": "这是带参数的get请求返回结果啊"
    }
  },

  {
    "description":"这是一个post请求啊",

    "request":{
      "uri":"/post",
      "method":"post",
      "forms":{
        "name":"xiaojing",
        "age":"27"
      }
    },

    "response":{
      "text":"这是post请求返回结果啊"
    }
  },

  {
    "description":"这是一个带cookies和headers的请求啊",

    "request":{
      "uri":"/cookies",
      "method":"post",
      "headers":{
        "Content-Type":"application/json"
      },
      "cookies":{
        "login":"true"
      },
      "json":{
        "name":"xiaojing",
        "age":"27"
      }
    },

    "response":{
      "status":200,
      "json":{
        "xiaojing":"true",
        "status":"1"}
    }

  },

  {
    "description":"重定向到百度",
    "request":{
      "uri":"/redirect"
    },
    "redirectTo":"https://www.baidu.com"
  },

  {
    "description":"重定向到path",
    "request":{
      "uri":"/redirectPath"
    },
    "redirectTo":"/xxj"
  }



